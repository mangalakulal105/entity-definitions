            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  3635617
                ],
                "query": "FROM Metric select average(aws.lex.RuntimeSuccessfulRequestLatency) as RuntimeSuccessfulRequestLatency since 1 week ago"
