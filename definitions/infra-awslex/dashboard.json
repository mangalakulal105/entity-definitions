{
  "name": "Amazon Lex",
  "description": null,
  "permissions": "PUBLIC_READ_WRITE",
  "pages": [
    {
      "name": "Amazon Lex",
      "description": null,
      "widgets": [
        {
          "title": "Audio/Video Logs Delivery Success/Failure",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  3635617
                ],
                "query": "SELECT count(`aws.lex.ConversationLogsTextDeliveryFailure`)as logsTextFailure, count(`aws.lex.ConversationLogsTextDeliverySuccess`)as logsTextSuccess, count(`aws.lex.ConversationLogsTextDeliverySuccess`)as logsTextSuccess, count(`aws.lex.ConversationLogsAudioDeliverySuccess`)as logsAudioSucces, count(`aws.lex.ConversationLogsAudioDeliveryFailure`)as logsAudioFailure FROM Metric SINCE 1 week AGO TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Runtime Request Counts",
          "layout": {
            "column": 5,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  3635617
                ],
                "query": "FROM Metric select count(aws.lex.RuntimeRequestCount) as RuntimeRequestCount where aws.Namespace='AWS/Lex' since 2 days ago"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Runtime Errors",
          "layout": {
            "column": 9,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  3635617
                ],
                "query": "FROM Metric select count(aws.lex.RuntimeUserErrors) as RuntimeUserErrors, count(aws.lex.RuntimeUserErrors) as RuntimeSystemErrors since 1 week ago timeseries"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "",
