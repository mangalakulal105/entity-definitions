{
  "name": "Dora Metrics Dashboard",
  "description": null,
  "pages": [
    {
      "name": "Dora Metrics Dashboard",
      "description": null,
      "widgets": [
        {
          "title": "Dora Metrics by Repo",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 12,
            "height": 3
          },
          "visualization": {
            "id": "viz.table"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT latest(deploymentFrequency), latest(leadTimeForChanges), latest(changeFailureRate), latest(timeToRestoreService) FROM DoraMetricsEvent FACET repoName"
              }
            ]
          }
        }
      ]
    }
  ]
}